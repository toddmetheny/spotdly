<div class="stats">
  <h5>Total Users: <%= User.count %></h5>
  <h5>Last Registered User: <%= User.last.email %></h5>
  <h5>Total Profiles: <%= Profile.count %></h5>
  <h5>Last Profile: <%= Profile.last.company_name %></h5>
</div>
<div class="user-emails col-md-6">
  <h3 class="text-center">User Emails</h3>

  <%= User.find_each do |user| %>
    <div class="text-center">
      <%= user.email %>
    </div>
  <% end %>
</div>
